void comm_out(unsigned char c)
{
  unsigned char i;
  digitalWrite(CS, LOW);
  digitalWrite(DC, LOW);
  for (i = 0; i < 8; i++)
  {
    digitalWrite(SCLK, LOW);
    if ((c & 0x80) >> 7 == 1)
    {
      digitalWrite(SDIN, HIGH);
    }
    else
    {
      digitalWrite(SDIN, LOW);
    }
    c = c << 1;
    digitalWrite(SCLK, HIGH);
  }
  digitalWrite(CS, HIGH);
  digitalWrite(DC, HIGH);
}

void data_out(unsigned char d)
{
  unsigned char i;
  digitalWrite(CS, LOW);
  for (i = 0; i < 8; i++)
  {
    digitalWrite(SCLK, LOW);
    if ((d & 0x80) >> 7 == 1)
    {
      digitalWrite(SDIN, HIGH);
    }
    else
    {
      digitalWrite(SDIN, LOW);
    }
    d = d << 1;
    digitalWrite(SCLK, HIGH);
  }
  digitalWrite(CS, HIGH);
}

void ImageTestForBanding_25664(const unsigned char array[])
{
  unsigned int i, j;

  Set_Column_Address_25664(28, 91);
  Set_Row_Address_25664(0, 63);
  comm_out(0x5C);

  for (i = 0; i < 2048; i++)
  {
    switch (pgm_read_byte_near(array[i]) >> 6)
    {
      case 0:
        data_out(0x00);
        break;
      case 1:
        data_out(0x0F);
        break;
      case 2:
        data_out(0xF0);
        break;
      case 3:
        data_out(0xFF);
        break;
      default:
        break;
    }
    switch ((array[i] >> 4) & 0x03)
    {
      case 0:
        data_out(0x00);
        break;
      case 1:
        data_out(0x0F);
        break;
      case 2:
        data_out(0xF0);
        break;
      case 3:
        data_out(0xFF);
        break;
      default:
        break;
    }
    switch ((array[i] >> 2) & 0x03)
    {
      case 0:
        data_out(0x00);
        break;
      case 1:
        data_out(0x0F);
        break;
      case 2:
        data_out(0xF0);
        break;
      case 3:
        data_out(0xFF);
        break;
      default:
        break;
    }
    switch ((array[i]) & 0x03)
    {
      case 0:
        data_out(0x00);
        break;
      case 1:
        data_out(0x0F);
        break;
      case 2:
        data_out(0xF0);
        break;
      case 3:
        data_out(0xFF);
        break;
      default:
        break;
    }
  }

}

